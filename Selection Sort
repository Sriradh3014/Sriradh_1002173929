arr = [58, 44, 9, 34, 20] 

print(f"Original array is - {arr}")
for i in range(len(arr)): 
    minimum_index = i 
    for j in range(i+1, len(arr)): 
        if arr[minimum_index] > arr[j]: 
            minimum_index = j 
    arr[i], arr[minimum_index] = arr[minimum_index], arr[i]
print (f"Sorted array - {arr}") 
  
